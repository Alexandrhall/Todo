<header>
    <div id="headwrap">
        <h2>To do List</h2>
        <input class="input" placeholder="Write New..." type="text" id="inp" />
        <button id="button">Add</button>
    </div>
</header>
<main id="mainwrap">
    <div class="butWrap">
        <button id="sort">Sort Name</button>
        <button id="dateSort">Sort Date</button>
    </div>
    <div class="listWrap">
        <h4>To do</h4>
        <ul id="myList" class="list">
            {{#each tasks}}
                <li>{{disc}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="listWrap">
        <h4>Done</h4>
        <ul id="doneList" class="list"></ul>
    </div>
</main>